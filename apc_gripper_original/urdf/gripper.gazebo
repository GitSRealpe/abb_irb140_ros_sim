<?xml version="1.0"?>
<robot>

    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/gripper</robotNamespace>
            <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        </plugin>


        <joint name="left_int__left_finger" type="revolute">
            <parent>left_int</parent>
            <child>left_finger</child>
            <pose>0.0 0.020785 0.012 ${pi} 0 0</pose>
            <axis>
                <xyz>1 0 0</xyz>
                <limit>
                    <lower>-3.14</lower>
                    <upper>3.14</upper>
                    <effort>1000</effort>
                    <velocity>0.5</velocity>
                </limit>
            </axis>
        </joint>

        <joint name="right_int__right_finger" type="revolute">
            <parent>right_int</parent>
            <child>right_finger</child>
            <pose>0.0 -0.020785 0.012 0 0 0</pose>
            <axis>
                <xyz>1 0 0</xyz>
                <limit>
                    <lower>-3.14</lower>
                    <upper>3.14</upper>
                    <effort>1000</effort>
                    <velocity>0.5</velocity>
                </limit>
            </axis>
        </joint>

    </gazebo>

</robot>
