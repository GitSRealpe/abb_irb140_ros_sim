		<child link="link_5" />
		<axis xyz="0 1 0" />
		<limit effort="1000" lower="-2.0071" upper="2.0071" velocity="6.2832" />
	</joint>
	<transmission name="trans_joint_5">
		<type>transmission_interface/SimpleTransmission</type>
